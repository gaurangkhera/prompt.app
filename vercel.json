{
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "must-revalidate" }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "s-maxage=1, stale-while-revalidate" }
        ]
      }
    ]
  }
  